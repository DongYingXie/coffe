<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="UniversalAnalytics">
        <param name="android-package" value="com.danielcwilson.plugins.analytics.UniversalAnalyticsPlugin" />
    </feature>
    <icon density="ldpi" src="android/drawable-ldpi/ic_launcher.png" />
    <icon density="mdpi" src="android/drawable-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="android/drawable-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="android/drawable-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="android/drawable-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="android/drawable-xxxhdpi/ic_launcher.png" />
    <name>HelloCordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
